<Layout xmlns="urn:speedata.de:2009/publisher/en"
    xmlns:sd="urn:speedata:2009/publisher/functions/en">
	<Pageformat height="5cm" width="6cm"></Pageformat>
    <DefineTextformat name="hspace" tab="hspace"></DefineTextformat>
    <Record element="data">
        <PlaceObject>
            <Textblock>
				<Paragraph>
					<Value select="."></Value>
				</Paragraph>
                <Paragraph textformat="hspace">
                    <Value select="."></Value>
                </Paragraph>
            </Textblock>
        </PlaceObject>
    </Record>
</Layout>