<Layout xmlns="urn:speedata.de:2009/publisher/en"
    xmlns:sd="urn:speedata:2009/publisher/functions/en"
    version="4.1.16">
    <Pageformat height="3cm" width="8cm" />
    <LoadFontfile
        name="Amiri-Regular"
        filename="amiri-regular.ttf"
        mode="harfbuzz" />
    <DefineFontfamily fontsize="10" leading="12" name="amiri">
        <Regular fontface="Amiri-Regular" />
    </DefineFontfamily>


    <Record element="data">
        <PlaceObject>
            <Textblock>
                <Paragraph bidi="yes" fontfamily="amiri">
                    <Value>العاشر ليونيكود (Unicode Conference)، الذي سيعقد في 10-12 آذار 1997 مبدينة</Value>
                </Paragraph>
            </Textblock>
        </PlaceObject>
    </Record>
</Layout>
