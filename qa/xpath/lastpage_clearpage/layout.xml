<Layout xmlns="urn:speedata.de:2009/publisher/en"
    xmlns:sd="urn:speedata:2009/publisher/functions/en">
    <Pageformat height="5cm" width="4cm" />
    <Record element="data">
        <PlaceObject>
            <Textblock>
                <Paragraph>
                    <Value select="$_lastpage"></Value>
                </Paragraph>
            </Textblock>
        </PlaceObject>
        <ClearPage />
    </Record>
</Layout>